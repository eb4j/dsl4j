<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <Match>
    <Source name="~.*/generated/.*\.java" />
  </Match>
  <Match>
    <Class name="io.github.eb4j.dsl.DslArticle"/>
    <Or>
      <Bug code="EI"/>
      <Bug code="EI2"/>
    </Or>
  </Match>
  <Match>
    <Class name="io.github.eb4j.dsl.DslResult"/>
    <Or>
      <Bug code="EI"/>
      <Bug code="EI2"/>
    </Or>
  </Match>
  <Match>
    <Or>
      <Class name="io.github.eb4j.dsl.DslIndexOuterClass"/>
      <Class name="io.github.eb4j.dsl.DslIndexOuterClass.DslIndex"/>
    </Or>
    <Or>
      <Bug code="EI"/>
      <Bug code="EI2"/>
      <Bug code="MS"/>
    </Or>
  </Match>
</FindBugsFilter>
